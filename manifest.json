{
    "name": "Calendar-Alert",
    "version": "1.0.0",
    "description": "Your calendar alert",
    "permissions":[
      "activeTab",
      "<all_urls>",
      "tabs"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "manifest_version": 3,
    "author": "Abigail and Evan",
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "css": ["style.css"],
          "js": ["openPop.js"]
        }
      ],
    "action":{
        "default-popup": "index.html",
        "default-title": "Your Next Calendar Event"
    }
}